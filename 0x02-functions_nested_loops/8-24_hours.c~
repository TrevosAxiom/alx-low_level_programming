#include "main.h"
/**
* jack_bauer - check the code
* void: void argument
* Return: VOID.
*/
void jack_bauer(void)
{

int hr, _hr, mt, _mt;

for (hr = 0; hr <= 2; hr++)
{
_hr = 0;
for (_hr = 0; _hr < 4; _hr++)
{
mt = 0;
for (mt = 0; mt < 5; mt++)
{
_mt = 0;
for (_mt = 0; _mt < 10; _mt++)
{
_putchar('0' + hr);
_putchar('0' + _hr);
_putchar(':');
_putchar('0' + mt);
_putchar('0' + _mt);
_putchar('\n');
}
}
}

}

}
